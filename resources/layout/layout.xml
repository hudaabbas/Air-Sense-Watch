    <layout id="Start">
        
        <bitmap id="rightTop" personality="
            system_icon_dark__hint_button_right_top
            system_loc__hint_button_right_top" />
    </layout>
    <layout id="UserPrompt" >
        <label id="pAQHI" x="center" y="150" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_WHITE" />
        <label id="blurb" text="@Strings.blurb" x="center" y="200" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_WHITE" />
   </layout>
    <layout id="ErrorLayout">
        <label id="Message" x="center" y="center" justification="Graphics.TEXT_JUSTIFY_CENTER" color="FFFFFF"/>
